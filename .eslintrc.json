{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "max-len": "off",
        "no-mixed-operators": "off",
        "max-classes-per-file": "off"
    },
    "globals": {
        "math": true
    }
}
